            #binding-cells = <1>;
            bindings =
                <&macro_press>,
                <&kp LSHFT>,
                <&macro_press>,
                <&kp LALT &macro_param_1to1>,
                <&macro_tap>,
                <&kp MACRO_PLACEHOLDER>,
                <&macro_release>,
                <&kp LALT>,
                <&macro_release>,
                <&kp LSHFT>,
                <&macro_tap>,
                <&kp DEL>;
        };

        emo_lol: macro_emoji_lol {
            #binding-cells = <0>;
            compatible = "zmk,behavior-macro";
            bindings =
                <&macro_tap>,
                <&kp LA(LS(U))>,
                <&macro_tap>,
                <&kp N1 &kp F>,
                <&macro_tap>,
                <&kp N9 &kp N2 &kp N3>;
        };

        emo_thu: macro_emoji_thumb {
            #binding-cells = <0>;
            compatible = "zmk,behavior-macro";
            bindings =
                <&macro_tap>,
                <&kp LA(LS(U))>,
                <&macro_tap>,
                <&kp N1 &kp F>,
                <&macro_tap>,
                <&kp N4 &kp N4 &kp D>;
