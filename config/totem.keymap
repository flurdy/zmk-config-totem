        emo_hap: macro_emoji_happy {
            #binding-cells = <0>;
            compatible = "zmk,behavior-macro";
            bindings =
                <&macro_tap>,
                <&kp LA(LS(U))>,
                <&macro_tap>,
                <&kp N1 &kp F>,
                <&macro_tap>,
                <&kp N6 &kp N0 &kp N4>;
