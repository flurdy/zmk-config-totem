            bindings =
                <&macro_tap>,
                <&kp LBKT>,
                <&macro_press>,
                <&kp LSHFT>,
                <&macro_tap>,
                <&kp F>,
                <&macro_release>,
                <&kp LSHFT>,
                <&macro_tap>,
                <&kp RBKT>;
        };

        mac_fun: macro_tag_fin {
            #binding-cells = <0>;
            compatible = "zmk,behavior-macro";
            bindings =
                <&macro_press>,
                <&kp LSHFT>,
                <&macro_tap>,
                <&kp F>,
                <&macro_release>,
                <&kp LSHFT>,
                <&macro_tap>,
                <&kp LBKT &kp UNDER &kp RBKT>;
        };

        mac_qot: macro_js_qoute {
            #binding-cells = <0>;
            compatible = "zmk,behavior-macro";
            bindings =
                <&macro_tap>,
                <&kp DQT &kp SPACE &kp DQT &kp COLON &kp SPACE &kp DQT &kp SPACE &kp DQT>;
        };

        mac_iqt: macro_int_qoute {
            #binding-cells = <0>;
            compatible = "zmk,behavior-macro";
            bindings =
                <&macro_tap>,
                <&kp S &kp DQT &kp SPACE &kp DQT>,
