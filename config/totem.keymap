                <&kp LEFT &kp LEFT>;
        };

        mac_bua: macro_brckund_arrow {
            #binding-cells = <0>;
            compatible = "zmk,behavior-macro";
            bindings = <&macro_tap>, <&kp UNDER &kp SPACE &kp EQUAL &kp GT>;
        };

        mac_dlb: macro_dollar_brack {
            #binding-cells = <0>;
            compatible = "zmk,behavior-macro";
            bindings =
