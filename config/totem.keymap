        emo_uha: macro_emoji_unhappy {
            #binding-cells = <0>;
            compatible = "zmk,behavior-macro";
            bindings =
                <&macro_tap>,
                <&kp LA(LS(U))>,
                <&macro_tap>,
                <&kp N1 &kp F>,
                <&macro_tap>,
                <&kp N6 &kp N4 &kp N1>;
        };

        emo_shr: macro_emoji_shrug {
            #binding-cells = <0>;
            compatible = "zmk,behavior-macro";
            bindings =
                <&macro_tap>,
                <&kp LA(LS(U))>,
                <&macro_tap>,
                <&kp N1 &kp F>,
                <&macro_tap>,
                <&kp N9 &kp N3 &kp N7>;
