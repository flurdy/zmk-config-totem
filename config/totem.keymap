#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/outputs.h>

#define BASE 0
#define NAVI 1
#define SYMB 2
#define NUMB 3
#define FUNC 4
#define CODE 5
#define MEDI 6
#define TILE 7
#define TSIZ 8
#define NORW 9
#define SELC 10
#define TERM 11
#define QWER 12
#define BLUE 13


&mt {
  quick-tap-ms = <100>;
  global-quick-tap;
  flavor = "tap-preferred";
  tapping-term-ms = <170>;
};

/ {

   behaviors {
        td_sl: tap_dance_sleep {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&none>, <&kp C_SLEEP>;
        };
        td_lc: tap_dance_lock {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&none>, <&kp K_LOCK>;
        };
        td_rs: tap_dance_reset {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&none>, <&sys_reset>;
        };
        td_bo: tap_dance_boot {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&none>, <&bootloader>;
        };
        td_spup: tap_dance_splitup {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp LA(LC(LS(UP)))>, <&codspl UP>;
        };
        td_spdn: tap_dance_splitdown {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp LA(LC(LS(DOWN)))>, <&codspl DOWN>;
        };
        mt_split: mod_tap_split {
            compatible = "zmk,behavior-hold-tap";
            label = "MT_SPLIT";
            #binding-cells = <2>;
            tapping-term-ms = <200>;
            quick-tap-ms = <0>;
            flavor = "hold-preferred";
            bindings = <&codspl>, <&kp>;
        };
    };

    macros {
        vsc: vscode_cmd {
            label = "VSCCMD";
            #binding-cells = <1>;
            compatible = "zmk,behavior-macro-one-param";
            bindings =
                <&kp LC(K)>,
                <&macro_param_1to1 &kp MACRO_PLACEHOLDER>;
        };
        codspl: macro_vscode_split {
            label = "VSCSPLIT";
            #binding-cells = <1>;
            compatible = "zmk,behavior-macro-one-param";
            bindings =
                <&kp LC(K)>,
                <&macro_param_1to1 &kp LS(MACRO_PLACEHOLDER)>;
        };
        mc_rm2: macro_rmeta2 {
            label = "MCRMETA2";
            compatible = "zmk,behavior-macro-two-param";
            #binding-cells = <2>;
            bindings =
               <&macro_press &kp RGUI>,
               <&macro_param_1to1 &kp MACRO_PLACEHOLDER>,
               <&macro_release &kp RGUI>,
               <&macro_param_2to1 &kp MACRO_PLACEHOLDER>;
        };
        mc_sldl: macro_select_delete {
            label = "MCSELDEL";
            compatible = "zmk,behavior-macro-one-param";
            #binding-cells = <1>;
            bindings =
               <&macro_press &kp LSHFT>,
               <&macro_param_1to1 &kp MACRO_PLACEHOLDER>,
               <&macro_release &kp LSHFT>,
               <&macro_tap &kp DEL>;
        };
        mc_csldl: macro_ctrsel_del {
            label = "MCCSLDEL";
            compatible = "zmk,behavior-macro-one-param";
            #binding-cells = <1>;
            bindings =
               <&macro_press &kp LSHFT>,
               <&macro_press &kp LCTRL>,
               <&macro_param_1to1 &kp MACRO_PLACEHOLDER>,
               <&macro_release &kp LCTRL>,
               <&macro_release &kp LSHFT>,
               <&macro_tap &kp DEL>;
        };

    };

        keymap {
                compatible = "zmk,keymap";
// ▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄

