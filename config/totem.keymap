                , <&macro_tap &kp K>
                , <&macro_release &kp LCTRL>
                , <&macro_tap &kp Z>
                ;
        };
        mc_dhm: macro_del_home {
            label = "DELHOME";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
